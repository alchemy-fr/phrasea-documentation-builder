{
  "name": "alchemy/doc-builder",
  "description": "A tool to prepare documentation files for Docusaurus.",
  "type": "project",
  "require": {
    "symfony/console": "^7.3",
    "symfony/filesystem": "^7.3",
    "symfony/http-client": "^7.3",
    "symfony/yaml": "^7.3",
    "symfony/process": "^7.3",
    "phlak/semver": "^6.1"
  },
  "scripts": {
    "cs": "php-cs-fixer fix ./src"
  },
  "autoload": {
    "psr-4": {
      "App\\": "./src"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.89"
  }
}
